	C6180 (User interfaces)},
  crossref = {ACM:1990:UTA},
  keywords = {Direct manipulation; Highly interactive direct manipulation interfaces;
	Interface types; Nonprogrammers; Object oriented paradigm; Product
	quality interfaces; QUICK; Structure editor; User-interface design
	kit},
  thesaurus = {Interactive systems; Object-oriented programming; Software tools;
	User interface management systems}
}

