  bibdate = {Wed Oct 7 09:18:19 MDT 2009},
  bibsource = {http://portal.acm.org/},
  coden = {CGRADI, CPGPBZ},
  doi = {http://dx.doi.org/10.1145/604446.604453},
  issn = {0097-8930},
  owner = {adrian},
  timestamp = {2013.12.18}
}

