@INPROCEEDINGS{Wiecha:1990:GUI,
  author = {C. Wiecha and S. Boies},
  title = {Generating user interfaces: principles and use of {ITS} style rules},
  year = {1990},
  pages = {21--30},
  abstract = {Historically, two types of layered architectures have been developed
	in attempts to provide the required flexibility in applications:
	user interface management systems (UIMS) and toolkits UIMS separate
	the application and its interface. Back end computations are separated
	from dialog control and style. Style, however, is often treated in
	a single interface layer. Toolkits separate style from the application.
	Dialog control remains in the back end while the implementation of
	interaction techniques is hidden in a code library. ITS is a four
	layer architecture with separate tools for back end computation,
	dialog control, rule-based interface style, and the implementation
	of graphic primitives used in the style. Recent work has focused
	particularly on the rule layer. The authors begin by reviewing previous
	work in UIMS and toolkits. Then they show how the rule layer in ITS
	combines the benefits of both UIMS and toolkits. (22 Refs.)},
  acknowledgement = {#ack-nhfb#},
  affiliation = {IBM Thomas J. Watson Res. Center, Yorktown Heights, NY, USA},
  bibdate = {Thu Dec 14 18:48:37 MST 1995},
  classification = {C6115 (Programming support); C6170 (Expert systems); C6180G (Graphical
	user interfaces)},
  crossref = {ACM:1990:UTA},
  keywords = {Back end computation; Dialog control; Graphic primitives; ITS style
	rules; Layered architectures; Rule-based interface style; Toolkits;
	UIMS; User interface management systems; User interfaces generation},
  thesaurus = {Graphical user interfaces; Knowledge based systems; Software tools;
	User interface management systems}
}

