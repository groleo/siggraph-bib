@INPROCEEDINGS{Blumenthal:1990:SAI,
  author = {B. Blumenthal},
  title = {Strategies for automatically incorporating metaphoric attributes
	in interface designs},
  year = {1990},
  pages = {66--75},
  abstract = {Recently a number of projects have addressed the problem of automatically
	producing interface designs for computer applications. The core of
	each of these design tools takes as input a specification of the
	functionality and information that the application is to present
	to the user and produces as output an interface specified in terms
	of some toolkit of interface techniques. Although there are a number
	of sets of guidelines for effective interface designs, they are all
	inadequate for a completely automated interface design system. The
	most obvious inadequacy is in the area of aesthetic decisions; most
	sets of interface guidelines either lack guidelines for determining
	the specific visual appearances and organization of an interface,
	or else such guidelines are too vague to be used automatically. Decisions
	about what aspects of the application to emphasize and what aspects
	to obscure are also poorly specified. Such decisions are implicit
	in any interface design, but guidelines for making them are vague
	at best, and often absent altogether. The MAID system addresses some
	of these inadequacies by using knowledge-based descriptions of real-world
	objects to guide decisions that are underspecified by a given set
	of design guidelines. (22 Refs.)},
  acknowledgement = {#ack-nhfb#},
  affiliation = {Dept. of Comput. Sci., Texas Univ. at Austin, TX, USA},
  bibdate = {Thu Dec 14 18:48:37 MST 1995},
  classification = {C6115 (Programming support); C6170 (Expert systems); C6180 (User interfaces)},
  crossref = {ACM:1990:UTA},
  keywords = {Aesthetic decisions; Automatically incorporating metaphoric attributes;
	Interface designs; Knowledge-based descriptions; MAID system; Real-world
	objects; Visual appearances},
  thesaurus = {Interactive systems; Knowledge based systems; Software tools; User
	interface management systems}
}

