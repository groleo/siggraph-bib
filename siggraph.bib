@INPROCEEDINGS{Calder:1990:GFO,
  author = {P. R. Calder and M. A. Linton},
  title = {Glyphs: flyweight objects for user interfaces},
  year = {1990},
  pages = {92--101},
  abstract = {Current user interface toolkits provide components that are complex
	and expensive. Programmers cannot use these components for may kinds
	of application data because the resulting implementation would be
	awkward and inefficient. The authors have defined a set of small,
	simple components, called glyphs, that programmers can use in large
	numbers to build user interfaces. To show that glyphs are simple
	and efficient, they have implemented a WYSIWYG document editor. The
	editor's performance is comparable to that of similar editors built
	with current tools, but its implementation is much simpler. (7 Refs.)},
