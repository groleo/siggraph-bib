@INPROCEEDINGS{Pittman:1990:VVU,
  author = {J. H. Pittman and C. J. Kitrick},
  title = {{VUIMS}: a visual user interface management system},
  year = {1990},
  pages = {36--46},
  abstract = {VUIMS is an object-oriented user interface management system that
	was designed to support reconfigurable components. VUIMS consists
	of a collection of objects and a semantically rich token language.
	The objects implement primitive presentation and interaction functions.
	The token language controls interaction and visual style. High level
	objects can be created from primitive objects using token templates.
	The user interface and application are controlled by token streams
	that are emitted in response to user actions. VUIMS supports a variety
	of presentation and interaction styles through simple, robust manipulation
	of a hierarchy of visual panels with a rich set of relationships
	and constraints. VUIMS has been used to implement two commercial
	high-performance computer graphics applications and an online help
	system. (15 Refs.)},
