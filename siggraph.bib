  coden = {CGRADI, CPGPBZ},
  issn = {0097-8930},
  journalabr = {Comput Graphics ACM},
  keywords = {Communication; Computer graphics; Computer simulation; Computer systems;
	Technology; Virtual reality; Virtual reality literacy}
